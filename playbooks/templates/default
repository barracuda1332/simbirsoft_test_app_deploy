##
server {
	listen 80 default_server;
	listen [::]:80 default_server;

	root /var/www/html;

	index index.html;

	server_name TestApp;

	location / {
		# proxy_pass http://127.0.0.1:5000;
		alias /var/www/html/static/;
                autoindex off;
	}

	location /api/ {
		proxy_pass http://127.0.0.1:5000/api/;
	}
}
